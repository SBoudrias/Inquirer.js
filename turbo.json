{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "tsc": {
      "dependsOn": ["^tsc"],
      "outputs": ["dist/**"],
      "inputs": ["$TURBO_DEFAULT$", "../../tools/fix-ext.mjs", "../../tsconfig.json"]
    },
    "attw": {
      "dependsOn": ["tsc"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
